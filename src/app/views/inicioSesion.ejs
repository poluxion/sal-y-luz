  <input type="number" class="form-control" id="cedula" placeholder="ejem: 1123123123.." name="cedula" autofocus>
</div>
	<div class="mb-3">
  <label for="formGroupExampleInput2" class="form-label">Contrase√±a: </label>
  <input type="password" class="form-control" id="pessw" name="pessw">
</div>

	<center>
	<button class="btn-login" type="submit">LOGIN</button>
  	<button class="btn-register"><a href="areaRegistro" class="btn-register">REGISTRO</a></button>
